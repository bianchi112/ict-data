		Scanner sc = new Scanner(System.in);

		do {
			System.out.println("\n*** 컬렉션 사용 테스트 ***\n");
			
			System.out.println("1. HashSet 사용 테스트");
			
			System.out.println("0. 끝내기");
		} while (true);
	}
}
