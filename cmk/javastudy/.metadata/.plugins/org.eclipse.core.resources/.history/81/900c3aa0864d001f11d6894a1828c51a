package Workshop4;

import java.util.Scanner;

public class Test05 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.print("입력 : ");
		int input = sc.nextInt();
		int sum = 0;

		if (input < 1 || input > 5) {
			System.out.println("1~5 사이의 숫자로 입력하세요.");
		} else {
			for (int i = 0; i <= 10; i++) {
				if (i % 3 != 0 && i % 5 != 0) {
					sum += i;
				}
			}
		}

	}
}
