package com.ict.day12;

public class Ex11 {

	public static void main(String[] args) {
		// 객체 생성과 상관없이 static 사용가능
		System.out.println(Ex10.num);
		
		// instance 이므로 오류
		// System.out.println(Ex10.su);
		
		// private 이므로 오류
		// System.out.println(Ex10.age);
		
		Ex10.play2();
		
		// instance 이므로 오류
		//Ex10.play();
		
		// private 이므로 오류
		// Ex10.play3();
		
	}

}
